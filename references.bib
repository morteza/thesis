  annotation = {1726 citations (Semantic Scholar/DOI) [2022-12-09]},
  file = {/Users/morteza/Documents/Zotero/storage/PTYI5IIJ/Dosenbach et al. - 2010 - Prediction of Individual Brain Maturity Using fMRI.pdf}
