@article{ratcliff2018,
  title = {Modeling {{Individual Differences}} in the {{Go}}/{{No-go Task}} with a {{Diffusion Model}}},
  author = {Ratcliff, Roger and {Huang-Pollock}, Cynthia and McKoon, Gail},
  year = {2018},
